AWS_ACCESS_KEY_ID=replace-with-real-value
AWS_SECRET_ACCESS_KEY=replace-with-real-value
AWS_REGION=replace-with-real-value
# S3 bucket for images
S3_BUCKET_NAME=replace-with-real-value

